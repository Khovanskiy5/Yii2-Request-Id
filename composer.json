{
  "name": "khovanskiy/yii2-request-id",
  "description": "Yii2 component for handling and propagating request IDs",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Khovanskiy",
      "email": "a@khovanskiy.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "khovanskiy\\yii2requestid\\": "src/"
    }
  },
  "require": {
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "~2.0.2",
    "yidas/yii2-bower-asset": "^2.0"
  },
  "extra": {
    "bootstrap": "khovanskiy\\yii2requestid\\RequestIdBootstrap"
  },
  "version": "1.0.2",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
